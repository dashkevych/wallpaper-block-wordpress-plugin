{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wallpaper/container",
  "version": "0.1.0",
  "title": "Wallpaper",
  "category": "widgets",
  "icon": "cover-image",
  "description": "Block that allows to create interactive and animated backgrounds.",
  "example": {},
  "supports": {
    "html": false,
    "anchor": true,
    "align": true,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    }
  },
  "attributes": {
    "minHeight": {
      "type": "number"
    },
    "minHeightUnit": {
      "type": "string"
    }
  },
  "textdomain": "wallpaper-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}